<!DOCTYPE html>

<html>
	<head>
		<title>{{ name }}</title>
	</head>
	
	<body>
		
	{{#name}}<h1>Name: {{name}}</h1>{{/name}}
	{{#address1}}<p>Address: {{address1}}</p>{{/address1}}
	{{#address2}}<p>Address: {{address2}}</p>{{/address2}}
	{{#city}}<p>City: {{city}}</p>{{/city}}
	{{#region}}<p>Region: {{region}}</p>{{/region}}
	{{#country}}<p>Country: {{country}}</p>{{/country}}
	{{#postalcode}}<p>Postal Code: {{postalcode}}</p>{{/postalcode}}
	{{#latitude}}<p>Latitude: {{latitude}}</p>{{/latitude}}
	{{#longitude}}<p>Longitude: {{longitude}}</p>{{/longitude}}
	{{#url}}<p>URL: {{url}}</p>{{/url}}
	{{#phone}}<p>Phone: {{phone}}</p>{{/phone}}
	{{#creationdate}}<p>Creation Date: {{creationdate}}</p>{{/creationdate}}
	{{#modificationdate}}<p>Modification Date: {{modificationdate}}</p>{{/modificationdate}}
	
	{{#UUID}}<p><a href="edit/{{UUID}}.html">{{/UUID}}Edit this entry</a></p>
	<p><a href="/">Back Home</a></p>
	
	</body>
</html>