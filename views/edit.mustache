<!DOCTYPE html>

<html>
	<head>
		<title>Editing: {{ name }}</title>
	</head>
	
	<body>
	<h1>{{name}}</h1>
	<form action="edit.php" method="post">
			<p><label>Venue Name:
				<input type="text" name="venuename" value="{{name}}">
			</label>
			</p>
			<p><label>Address:
				<input type="text" name="address1" value="{{address1}}">
			</label></p>
			<p><label>Address: 
				<input type="text" name="address2" value="{{address2}}"></p>
			</label>
			</p>
			<p><label>City:
				<input type="text" name="city" value="{{city}}">
			</label>
			</p>
			<p><label>State/Region:
				<input type="text" name="region" value="{{region}}">
			</label>
			</p>
			<p><label>Country:
				<input type="text" name="country" value="USA" placeholder="{{country}}">
			</label>
			</p>
			<p><label>Postal Code:
				<input type="text" name="postalcode" value="{{postalcode}}">
			</label>
			</p>
			<p><label>Latitude:
				<input type="text" name="latitude" value="{{latitude}}">
			</label>
			</p>
			<p><label>Longitude:
				<input type="text" name="longitude" value="{{longitude}}">
			</label>
			</p>
			<p><label>Website URL:
				<input type="text" name="url" value="{{url}}">
			</label>
			</p>
			<p><label>Phone Number:
				<input type="text" name="phone" value="{{phone}}">
			</label>
			</p>
			<p><label>Type:
				<input type="text" name="type" value="{{type}}">
			</label>
			</p>
			<input type="hidden" name="UUID" value="{{UUID}}">
			<input type="submit" value="Submit">
		</form>	
	
	
	
	</body>
</html>